{
  "name": "pimcore/frontend-permission-toolkit-bundle",
  "description": "Provides a way to configure permissions for frontend applications in data objects.",
  "license": "GPL-3.0+",
  "type": "pimcore-bundle",
  "autoload": {
    "psr-4": {
      "FrontendPermissionToolkitBundle\\": "src/"
    }
  },
  "prefer-stable": true,
  "require": {
    "pimcore/pimcore": "^11.0",
    "symfony/config": "^6.1",
    "symfony/dependency-injection": "^6.1",
    "symfony/event-dispatcher": "^6.1",
    "symfony/event-dispatcher-contracts": "^3.0",
    "symfony/security-core": "^6.1"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10"
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "FrontendPermissionToolkitBundle\\FrontendPermissionToolkitBundle"
      ]
    }
  }
}
