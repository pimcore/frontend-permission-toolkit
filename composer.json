{
  "name": "pimcore/frontend-permission-toolkit-bundle",
  "description": "Provides a way to configure permissions for frontend applications in data objects.",
  "license": "GPL-3.0+",
  "type": "pimcore-bundle",
  "autoload": {
    "psr-4": {
      "FrontendPermissionToolkitBundle\\": "src/"
    }
  },
  "require": {
    "pimcore/pimcore": "^10.5",
    "symfony/config": "^5.3",
    "symfony/dependency-injection": "^5.3.3",
    "symfony/event-dispatcher": "^5.4.0",
    "symfony/event-dispatcher-contracts": "^2.2",
    "symfony/security-core": "^5.4.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.9"
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "FrontendPermissionToolkitBundle\\FrontendPermissionToolkitBundle"
      ]
    }
  }
}
